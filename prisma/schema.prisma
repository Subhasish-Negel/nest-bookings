generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Bookings {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  title     String   @unique
  advance   Int?     @default(0)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
